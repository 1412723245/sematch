PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX jps: <http://example.org/JobPositionSeeker.rdfs#>
PREFIX skills: <http://example.org/it-skills.rdfs#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

CONSTRUCT {
	?res jps:skill ?skill .
	?res jps:level ?level
} WHERE {

?candiate rdf:about ?res .
?res jps:skill ?skill .
?res jps:level ?level 
}