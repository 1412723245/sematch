PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX jps: <http://example.org/JobPositionSeeker.rdfs#>
PREFIX skills: <http://example.org/it-skills.rdfs#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?jps ?salary ?travel ?skill ?level WHERE {

?jps rdf:type jps:JobPositionSeeker .

?jps jps:desiredSalaryBase ?salary .

?jps jps:travelDesired ?travel .

?jps jps:hasSkill ?z .

?z rdf:type ?skill .

?z skills:competenceLevel ?level

}